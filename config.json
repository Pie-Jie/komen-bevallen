{
  "stylesheets": {
    "src":{
      "folder": "_scss",
      "file":"**/*.scss"
    },
    "dest": {
      "folder":"public/css",
      "file":"screen.css"
    }
  },
  "scripts": {
    "src":{
      "folder": "_js",
      "file":"**/*.js",
      "entry_file":"app.js"
    },
    "dest": {
      "folder":"public/js",
      "file":"app.js"
    }
  },
  "shared": {
    "src":{
      "folder": "classes",
      "file":"**/*.js"
    }
  },
  "templates": {
    "src":{
      "folder": "_hbs",
      "file":"*.hbs"
    },
    "dest": {
      "folder":"public/js",
      "file":"app.js"
    }
  }
}
